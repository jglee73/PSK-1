  //-----------------------------
  //	Selected Equipment Status Request (SSR)   	H -> E

  #MESSAGE    S1F3
  #REPLY      1
  #DIRECTION  RECEIVE    
  #TYPE	      0
  
#UNFORMATTED
  1.<L,  1  $LIST_LIMIT
         1.<U4[1]   $SVID
  
  #END
  //-----------------------------
  //-----------------------------
  // 	Selected Equipment Status Data (SSD)   		H <- E	

  #MESSAGE    S1F4  
  #DIRECTION  SEND  

#UNFORMATTED
  1.<L,  1  $LIST_LIMIT
         1.<V   $V

  #END
  //-----------------------------

  //-----------------------------
  // 	Status Variable Namelist (SVNR)    		H -> E

  #MESSAGE    S1F11
  #REPLY      1
  #DIRECTION  RECEIVE
  #TYPE	      0  

#UNFORMATTED  
  1.<L,  1    $LIST_LIMIT
         1.<U4[1]   $SVID 
	  
  #END
  //-----------------------------
  //-----------------------------
  // 	Status Variable Namelist Reply (SVNRR)   	H <- E		

  #MESSAGE    S1F12  
  #DIRECTION  SEND   
  
#UNFORMATTED
  1.<L,  1    $LIST_LIMIT
   	 1.<L,  3 
   	        1.<U4[1]   $SVID  
   	        2.<A[20]   $SVNAME 
   	        3.<A[10]   $UNITS  
  
  #END
