  #REPLY      1
  #DIRECTION  SEND       
  
  1.<L,  2
#UNFIXED  
   	 1.<A[1]   MDLN
#UNFIXED  
   	 2.<A[1]   SOFTREV
   	
  #END
  //-----------------------------
   //-----------------------------
  // 	Establish Communications Request Ack.(CRA)  	H -> E	

  #MESSAGE    S1F14  
  #DIRECTION  RECEIVE    
  
  1.<L,  2
   	 1.<B[1]  $COMMACK
   	 2.<L,  0
  
  #END
  //-----------------------------
 
  //-----------------------------
  // 	Request OFF-LINE (ROFL)    			H -> E	

  #MESSAGE    S1F15  
  #REPLY      1
  #DIRECTION  RECEIVE   

  #END
  //-----------------------------
  //-----------------------------
  // 	OFF-LINE Acknowledge(OFLA)    			H <- E	

  #MESSAGE    S1F16  
  #DIRECTION  SEND 
  
  1.<B[1]   $OFLACK
    
  #END
  
/*
OFLACK
	0 = OFF-LINE Acknowledge
	1 ~ 63 = Reserved
*/  
  //-----------------------------
  
  //-----------------------------
  // 	Request ON-LINE (RONL)    			H -> E

  #MESSAGE    S1F17
  #REPLY      1
  #DIRECTION  RECEIVE
  
  #END
  //-----------------------------
  //-----------------------------
  // 	ON-LINE acknowledge (ONLA)   			H <- E	

  #MESSAGE    S1F18  
  #DIRECTION  SEND 
  
  1.<B[1]   $ONLACK 
  
  #END
  
/*
ONLACK
	0 = ON-LINE Accepted
	1 = ON-LINE Not Allowed
	2 = Equipment Already ON-LINE
	3 ~ 63 = Reserved
*/  
  //-----------------------------


