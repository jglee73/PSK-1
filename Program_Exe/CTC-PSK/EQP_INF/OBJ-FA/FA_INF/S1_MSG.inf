  #REPLY      1
  #DIRECTION  RECEIVE  
  
  1.<L,  0
  
  #END
  //-----------------------------
  //-----------------------------
  // 	Establish Communications Request Ack.(CRA)  	H <- E	

  #MESSAGE    S1F14  
  #DIRECTION  SEND     
  
  1.<L,  2
   	 1.<B[1]   $COMMACK 
   	 2.<L,  2
#UNFIXED
   	        1.<A[1]   MDLN
